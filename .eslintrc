{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "consistent-return": 0,
    "react/no-multi-comp": [2, { "ignoreStateless": true }],
    "react/jsx-wrap-multilines": 0,
    "class-methods-use-this": 0,
    "react/no-danger": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [2, {ignore: ["^components", "gatsby-helpers", "config"]}],
    "import/extensions": 0,
  }
}
